{
    "name": "jotaelesalinas/php-mapreduce",
    "type": "library",
    "description": "A local implementation of the map-reduce strategy in PHP",
    "keywords": [
        "php-mapreduce",
        "php",
        "mapreduce",
        "map-reduce",
        "map",
        "reduce",
        "non-distributed"
    ],
    "homepage": "https://github.com/jotaelesalinas/php-mapreduce",
    "license": "MIT",
    "authors": [
        {
            "name": "JosÃ© Luis Salinas",
            "email": "jotaelesalinas@example.com",
            "homepage": "http://github.com/jotaelesalinas",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "MapReduce\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MapReduce\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "format": "phpcbf --standard=psr2 src/ tests/",
        "format-win": "phpcbf --standard=psr2 --no-patch src/ tests/"
    },
    "config": {
        "sort-packages": true
    }
}
